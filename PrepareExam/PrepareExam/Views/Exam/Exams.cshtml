@using PrepareExam.Models.ViewModels
@model List<ExamPartsViewModel>
@{
    ViewData["Title"] = "Sınavlar";
    int i = 0;
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="#">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Sınavlar</li>
    </ol>
</nav>

<table class="table align-middle">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Başlık</th>
            <th scope="col">Tarih</th>
            <th scope="col">##</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            i++;
            <tr>
                <th scope="row">@i</th>
                <td>@item.Blog.Title</td>
                <td>@item.CreateDate</td>
                <td>
                    <a href="/Exam/DeleteExam/@item.Id" class="btn btn-danger btn-sm px-3"><i class="fa fa-times"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>

<script type="text/javascript">

</script>